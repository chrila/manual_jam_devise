<h1>User admin</h1>
<h2>Users:</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>email</th>
      <th># stories</th>
      <th>Actions</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.stories.length %></td>
        <td>
          <%= link_to "Delete", user_registration_id_path(user), method: "delete" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>